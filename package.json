{
  "name": "nest-boilerplate-v1",
  "version": "0.0.1",
  "description": "",
  "type": "commonjs",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:production": "NODE_ENV=production nest start --watch",
    "start:stag": "NODE_ENV=staging nest start --debug --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config orm-config",
    "typeorm:prod": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./dist/orm-config.js",
    "typeorm:generate": "npm run typeorm migration:generate -- -n",
    "typeorm:generate:prod": "NODE_ENV=production npm run typeorm:prod migration:generate -- -n",
    "typeorm:push": "npm run typeorm migration:run",
    "typeorm:push:prod": "NODE_ENV=production npm run typeorm:prod migration:run"
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.3.1",
    "@grpc/grpc-js": "^1.6.8",
    "@grpc/proto-loader": "^0.7.0",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.4.4",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^9.1.6",
    "@nestjs/platform-ws": "^10.2.2",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^9.1.6",
    "@types/nodemailer": "^6.4.4",
    "@types/socket.io": "^3.0.2",
    "@types/sshpk": "^1.17.1",
    "amqp-connection-manager": "^4.1.3",
    "amqplib": "^0.9.1",
    "bcrypt": "^5.0.1",
    "bullmq": "^1.86.5",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "chance": "^1.1.8",
    "class-transformer": "^0.5.1",
    "class-validator": "0.13.2",
    "cron": "^2.1.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.1",
    "exceljs": "^4.3.0",
    "js-yaml": "^4.1.0",
    "jwks-client": "^2.0.2",
    "jwks-rsa": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "nest-winston": "^1.8.0",
    "passport": "^0.5.3",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "postgres": "^1.0.2",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.5",
    "socket.io-client": "^4.7.2",
    "sshpk": "^1.17.0",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.2.45",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "winston": "^3.8.2",
